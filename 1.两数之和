def twoSum(nums, target):
    flag = 0
    for i in range(0,len(nums)-1):
        j = i + 1
        for j in range(0,len(nums)-1):
            if nums[i] + nums[j] == target:
                print (i, j)
                flag = 1
    if flag == 0:
        print ('error')

#nums = [10, 20, 30, 40, 50, 60, 90 , 100]
#target = 90
